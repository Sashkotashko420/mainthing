	ButtonHolder.Name = "ButtonHolder"
	ButtonHolder.Parent = ScriptHubFrame
	ButtonHolder.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
	ButtonHolder.BackgroundTransparency = 1.000
	ButtonHolder.Position = UDim2.new(0, 0, 0.117000043, 0)
	ButtonHolder.Size = UDim2.new(0, 203, 0, 322)

	UIListLayout_2.Parent = ButtonHolder
	UIListLayout_2.SortOrder = Enum.SortOrder.LayoutOrder
	UIListLayout_2.Padding = UDim.new(0, 3)

	DexButton.Name = "DexButton"
	DexButton.Parent = ButtonHolder
	DexButton.BackgroundColor3 = Color3.fromRGB(20, 20, 20)
	DexButton.Size = UDim2.new(0, 204, 0, 36)
	DexButton.Font = Enum.Font.SourceSans
	DexButton.Text = "Dark Dex v4"
	DexButton.TextColor3 = Color3.fromRGB(240, 240, 240)
	DexButton.TextScaled = true
	DexButton.TextSize = 14.000
	DexButton.TextWrapped = true
	DexButton.MouseButton1Click:Connect(function()
		loadstring(game:HttpGet('https://gist.githubusercontent.com/DinosaurXxX/b757fe011e7e600c0873f967fe427dc2/raw/ee5324771f017073fc30e640323ac2a9b3bfc550/dark%2520dex%2520v4'))()
	end)

	UICorner_15.Parent = DexButton

	RemoteSpyButton.Name = "RemoteSpyButton"
	RemoteSpyButton.Parent = ButtonHolder
	RemoteSpyButton.BackgroundColor3 = Color3.fromRGB(20, 20, 20)
	RemoteSpyButton.Size = UDim2.new(0, 204, 0, 36)
	RemoteSpyButton.Font = Enum.Font.SourceSans
	RemoteSpyButton.Text = "Simple Spy v2"
	RemoteSpyButton.TextColor3 = Color3.fromRGB(240, 240, 240)
	RemoteSpyButton.TextScaled = true
	RemoteSpyButton.TextSize = 14.000
	RemoteSpyButton.TextWrapped = true
	RemoteSpyButton.MouseButton1Click:Connect(function()
		loadstring(game:HttpGet("https://github.com/exxtremestuffs/SimpleSpySource/raw/master/SimpleSpy.lua"))()
	end)

	UICorner_16.Parent = RemoteSpyButton
	
	local Holder = {}
	
	function Holder:Script(name, callback)
		local ScriptButton = Instance.new("TextButton")

		ScriptButton.Name = "ScriptButton"
		ScriptButton.Parent = ScriptsHolder
		ScriptButton.BackgroundColor3 = Color3.fromRGB(15, 15, 15)
		ScriptButton.Size = UDim2.new(0, 151, 0, 31)
		ScriptButton.Font = Enum.Font.GothamBold
		if name == nil then
			ScriptButton.Text = "Script Button"
		else
			ScriptButton.Text = name
		end
		ScriptButton.TextColor3 = Color3.fromRGB(240, 240, 240)
		ScriptButton.TextScaled = true
		ScriptButton.TextSize = 14.000
		ScriptButton.TextWrapped = true
		ScriptButton.MouseButton1Click:Connect(function()
			pcall(callback)
		end)
		
		UICorner_8.Parent = ScriptButton
	end
	return Holder
end

return Executor
