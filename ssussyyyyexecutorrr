	ScriptHubTitle.Name = "ScriptHubTitle"
	ScriptHubTitle.Parent = ScriptHubFrame
	ScriptHubTitle.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
	ScriptHubTitle.BackgroundTransparency = 1.000
